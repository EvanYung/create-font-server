{
  "name": "create-font-server",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "rimraf ./dist/* && tsup ./src/app.ts --dts --format cjs,esm  ",
    "start": "cross-env NODE_ENV=production npx pm2 start ecosystem.config.js",
    "restart": "cross-env NODE_ENV=production npx pm2 restart ecosystem.config.js",
    "stop": "npx pm2 stop ecosystem.config.js",
    "delete": "npx pm2 delete ecosystem.config.js",
    "logs": "npx pm2 logs",
    "list": "npx pm2 ls -a",
    "type:check": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "consola": "^3.2.3",
    "font-carrier": "^0.3.1",
    "fs-extra": "^11.2.0",
    "koa": "^2.15.0",
    "koa-async-validator": "^0.4.1",
    "koa-body": "^6.0.1",
    "koa-helmet": "^7.0.2",
    "koa-logger": "^3.2.1",
    "koa-route": "^3.2.0",
    "koa-router": "^12.0.1",
    "koa-static": "^5.0.0",
    "koa-websocket": "^7.0.0",
    "koa2-cors": "^2.0.6",
    "mysql2": "^3.9.1",
    "reflect-metadata": "^0.2.1",
    "superstruct": "^1.0.3",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/koa": "^2.14.0",
    "@types/koa-logger": "^3.1.5",
    "@types/koa-route": "^3.2.8",
    "@types/koa-router": "^7.4.8",
    "@types/koa-static": "^4.0.4",
    "@types/koa-websocket": "^5.0.10",
    "@types/koa2-cors": "^2.0.5",
    "@types/node": "^20.11.19",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.4",
    "element-plus": "^2.6.2",
    "nodemon": "^3.0.3",
    "pm2": "^5.3.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vue": "^3.4.21"
  }
}
